{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false,
      "autoApprove": [
        "sequentialthinking",
        "sequentialthinking"
      ],
      "disabledTools": [
        "sequentialthinking"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "disabled": false,
      "autoApprove": [
        "create_entities",
        "add_observations",
        "create_relations",
        "read_graph"
      ]
    },
    "fast-filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "fast-filesystem-mcp"
      ],
      "env": {
        "CREATE_BACKUP_FILES": "true"
      },
      "disabled": false
    }
  }
}